{
    "name": "tightenco/quicksand",
    "description": "Easily schedule regular cleanup of old soft-deleted Eloquent data.",
    "homepage": "https://github.com/tightenco/quicksand",
    "license": "MIT",
    "authors": [
        {
            "name": "Benson Lee",
            "email": "benson@tighten.co"
        },
        {
            "name": "Matt Stauffer",
            "email": "matt@tighten.co"
        }
    ],
    "require": {
        "illuminate/config": "~5.6",
        "illuminate/console": "~5.6",
        "illuminate/log": "~5.6",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "illuminate/database": "~5.6",
        "mockery/mockery": "^1.0.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\Quicksand\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\Quicksand\\QuicksandServiceProvider"
            ]
        }
    }
}
